{
  "name": "Indent Space",
  "namespace": "Dcc\\IndentSpace",
  "description": "Indent skip patterns",
  "documentation": "README.md",
  "authors": [
    {
      "name": "Greg Neils",
      "email": "gn2003@cumc.columbia.edu",
      "institution": "Columbia University"
    }
  ],
  "permissions": [
    "redcap_data_entry_form",
    "redcap_survey_page"
  ],
  "enable-every-page-hooks-on-system-pages": false,
  "framework-version": 6,
  "system-settings": [
    {
      "key": "system-readme",
      "name": "This EM provides an easy way to indent questions by a set amount.  See Readme for examples.",
      "type": "descriptive"
    },
    {
      "key": "indent-size",
      "name": "Base amount to indent.  Suggested 40.",
      "type": "text",
      "validation": "integer"
    }
  ],
  "compatibility": {
    "php-version-min": "7.2",
    "php-version-max": "",
    "redcap-version-min": "11.0",
    "redcap-version-max": ""
  }
}